{
  "manifest_version": 2,

  "name": "DZGagger",
  "description": "Hosting gags to imgur",
  "version": "0.1",

  "permissions": [
  	  "http://9gag.com/","https://*.imgur.com/","proxy"
  ],
   "content_scripts": [ {
    "js": [ "js/jquery.js","js/bootstrap.min.js"],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "browser_action": {
    "default_title":"DZGagger",
    "default_icon": "img/icon.png",
    "default_popup": "main.html"
  }
}